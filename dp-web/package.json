{
  "name": "dp-web",
  "version": "0.0.1",
  "private": true,
  "engines": {
    "node": ">=7.0.0"
  },
  "angular-cli": {},
  "scripts": {
    "start": "ng serve",
    "dev": "ng serve --host 0.0.0.0 --proxy-config proxy.conf.json",
    "dev:hmr": "ng serve --hmr -e=dev-hmr --host 0.0.0.0 --proxy-config proxy.conf.json",
    "prod": "ng serve --target=production --host 0.0.0.0 --proxy-config proxy.conf.json",
    "build": "ng build --target=production --sourcemaps=true",
    "prettier": "prettier --write \"./src/**/*.ts\"",
    "lint": "tslint \"src/**/*.ts\"",
    "test": "ng test",
    "pree2e": "webdriver-manager update",
    "e2e": "protractor",
    "pree2e:flakey": "webdriver-manager update",
    "e2e:flakey": "protractor-flake --parser standard  --max-attempts=3 -- protractor.conf.js",
    "report:licenses": "nlf"
  },
  "dependencies": {
    "@angular/common": "^4.0.0",
    "@angular/compiler": "^4.0.0",
    "@angular/compiler-cli": "^4.0.0",
    "@angular/core": "^4.0.0",
    "@angular/forms": "^4.0.0",
    "@angular/http": "^4.0.0",
    "@angular/platform-browser": "^4.0.0",
    "@angular/platform-browser-dynamic": "^4.0.0",
    "@angular/router": "^4.0.0",
    "@ngui/auto-complete": "^0.13.2",
    "@ngx-translate/core": "^6.0.1",
    "@ngx-translate/http-loader": "0.0.3",
    "angular-star-rating": "^3.0.8",
    "core-js": "^2.4.1",
    "d3": "~3.5.3",
    "d3-tip": "^0.7.1",
    "dagre-d3": "^0.4.17",
    "dialog-polyfill": "^0.4.9",
    "dps-apps": "git+ssh://git@github.com/hortonworks/dps-apps.git#>0.1.4-alpha.1",
    "font-awesome": "^4.7.0",
    "leaflet": "^1.0.2",
    "leaflet-curve": "^1.0.0",
    "material-design-icons": "^3.0.1",
    "material-design-lite": "^1.3.0",
    "moment": "^2.18.1",
    "nvd3": "^1.8.5",
    "rxjs": "5.1.0",
    "ts-helpers": "^1.1.1",
    "zone.js": "^0.8.4"
  },
  "devDependencies": {
    "@angular/cli": "^1.0.0",
    "@angularclass/hmr": "^2.1.3",
    "@types/d3": "~3.5.3",
    "@types/d3-tip": "^3.5.3",
    "@types/dagre-d3": "^0.4.17",
    "@types/jasmine": "^2.6.2",
    "@types/jasminewd2": "^2.0.3",
    "@types/leaflet": "^1.0.53",
    "@types/leaflet-curve": "0.0.29",
    "@types/nvd3": "^1.8.34",
    "codelyzer": "~3.0.1",
    "jasmine-core": "2.4.1",
    "jasmine-spec-reporter": "2.5.0",
    "karma": "1.2.0",
    "karma-chrome-launcher": "^2.0.0",
    "karma-cli": "^1.0.1",
    "karma-coverage-istanbul-reporter": "^1.0.0",
    "karma-jasmine": "^1.0.2",
    "karma-jasmine-html-reporter": "^0.2.2",
    "karma-remap-istanbul": "^0.2.1",
    "mydaterangepicker": "^4.1.9",
    "nlf": "^2.0.0",
    "prettier": "^1.6.1",
    "protractor": "^5.1.0",
    "protractor-flake": "^3.0.1",
    "roboto-npm-webfont": "0.0.4",
    "shelljs": "^0.7.8",
    "ts-node": "1.2.1",
    "tslint": "^5.2.0",
    "typescript": "~2.2.2"
  },
  "prettier": {
    "printWidth": 180,
    "tabWidth": 2,
    "semi": true,
    "bracketSpacing": true,
    "trailingComma": "none",
    "singleQuote": true,
    "parser": "typescript"
  }
}
