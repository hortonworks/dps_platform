{
  "name": "dp-web",
  "version": "0.0.1",
  "private": true,
  "engines": {
    "node": ">=7.0.0"
  },
  "angular-cli": {},
  "scripts": {
    "start": "ng serve",
    "dev": "ng serve --host 0.0.0.0 --proxy-config proxy.conf.json",
    "dev:hmr": "ng serve --hmr -e=dev-hmr --host 0.0.0.0 --proxy-config proxy.conf.json",
    "prod": "ng serve --target=production --host 0.0.0.0 --proxy-config proxy.conf.json",
    "build": "ng build --prod --source-map  --build-optimizer=false",
    "prettier": "prettier --write \"./src/**/*.{ts,json,css}\"",
    "lint": "tslint \"src/**/*.ts\"",
    "test": "ng test",
    "pree2e": "webdriver-manager update",
    "e2e": "protractor",
    "pree2e:flakey": "webdriver-manager update",
    "e2e:flakey": "protractor-flake --parser standard  --max-attempts=3 -- protractor.conf.js",
    "report:licenses": "nlf"
  },
  "dependencies": {
    "@angular/animations": "^6.0.0",
    "@angular/common": "^6.0.0",
    "@angular/compiler": "^6.0.0",
    "@angular/core": "^6.0.0",
    "@angular/forms": "^6.0.0",
    "@angular/http": "^6.0.0",
    "@angular/platform-browser": "^6.0.0",
    "@angular/platform-browser-dynamic": "^6.0.0",
    "@angular/router": "^6.0.0",
    "@ngui/auto-complete": "^0.13.2",
    "@ngx-translate/core": "^10.0.1",
    "@ngx-translate/http-loader": "^3.0.1",
    "core-js": "^2.5.4",
    "dialog-polyfill": "^0.4.9",
    "dps-apps": "git+ssh://git@github.com/hortonworks/dps-apps.git#0.1.4-alpha.16",
    "font-awesome": "^4.7.0",
    "leaflet": "^1.3.1",
    "leaflet-curve": "^1.0.0",
    "material-design-icons": "^3.0.1",
    "material-design-lite": "^1.3.0",
    "moment": "^2.18.1",
    "rxjs": "6",
    "rxjs-compat": "6",
    "ts-helpers": "^1.1.1",
    "zone.js": "^0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.6.1",
    "@angular/cli": "~6.0.1",
    "@angular/compiler-cli": "^6.0.0",
    "@angular/language-service": "^6.0.0",
    "@angularclass/hmr": "^2.1.3",
    "@types/jasmine": "~2.8.6",
    "@types/jasminewd2": "~2.0.3",
    "@types/leaflet": "^1.2.7",
    "@types/leaflet-curve": "^0.1.1",
    "@types/node": "~8.9.4",
    "codelyzer": "~4.2.1",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~1.7.1",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~1.4.2",
    "karma-jasmine": "~1.1.1",
    "karma-jasmine-html-reporter": "^0.2.2",
    "prettier": "^1.6.1",
    "protractor": "~5.3.0",
    "protractor-flake": "^3.0.1",
    "roboto-npm-webfont": "0.0.4",
    "shelljs": "^0.7.8",
    "ts-node": "~5.0.1",
    "tslint": "~5.9.1",
    "typescript": "~2.7.2"
  },
  "prettier": {
    "printWidth": 180,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "all",
    "parser": "typescript"
  }
}
